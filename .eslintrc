{
   "extends": "eslint:recommended",
   "parser": "babel-eslint",
   "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true,
         "react/jsx-filename-extensions": 0
      }
   },
   "env": {
      "node": true,
      "browser": true,
      "es6": true
   },
   "rules": {
      "comma-dangle" : [
         "error",
         "never"
      ],
      "no-unused-vars" : [
         0,
         {
            "vars": "local",
            "args": "none"
         }
      ],
      "no-extra-semi": 0,
      "no-console": 1
   }
}